# http://editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = tab
insert_final_newline = true
tab_width = 4
trim_trailing_whitespace = true

# Ignore vendor path
[vendor/**]
root = unset
charset = none
end_of_line = none
indent_style = none
insert_final_newline = none
tab_width = none
trim_trailing_whitespace = none
