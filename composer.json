{
	"name": "syslogic/document-export-definition",
	"type": "library",
	"description": "Document Export Definition for Scienta Library and conversion servers",
	"homepage": "https://www.scienta.nl",
	"license": "MIT",
	"authors": [
		{"name": "Scienta", "email": "info@scienta.nl"}
	],
	"minimum-stability": "dev",
	"require": {
		"php": "~7.1 || ~8.0",
		"jms/serializer": "*"
	},
	"require-dev": {
		"slevomat/coding-standard": "^7.0",
		"squizlabs/php_codesniffer": "dev-master",
		"vimeo/psalm": "^4.18",
		"phpcompatibility/php-compatibility": "*"
	},
	"autoload": {
		"psr-0": { "DocumentExportDefinition\\": "." }
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
